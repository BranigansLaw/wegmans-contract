<in>
	<table>
		<title>Store Inventory History {3:yyyyMMdd}</title>
		<sdesc>SnapShot of Store OnHands Qty and Comitted Qty as stated in ERx</sdesc>
		<cols>
			<th name="date_of_service" type="i" format="type:date;width:8;dec:0">Date</th>
			<th name="store_num" type="i" format="type:nocommas;width:3;dec:0">Store #</th>
			<th name="ndc_wo" type="a" format="type:char;width:11">NDCwo</th>
			<th name="drug_name" type="a" format="type:char;width:30">Drug Name</th>
			<th name="sdgi" type="a" format="type:char;width:1">SDGI</th>
			<th name="gcn" type="a" format="type:char;width:7">GCN</th>
			<th name="gcn_seq_num" type="f" format="type:nocommas;width:8;dec:2">GCN Seq</th>
			<th name="orange_book_code" type="a" format="type:char;width:3">OBC</th>
			<th name="form_code" type="a" format="type:char">Form Code</th>
			<th name="pack_size" type="f" format="type:num;dec:2">Pack Size</th>
			<th name="true_pack" type="f" format="type:num;dec:2">True Pack</th>
			<th name="pm" type="a" format="type:char;width:1">PM</th>
			<th name="is_preferred" type="a" format="type:char;width:1">P</th>
			<th name="lastacqcost_pack" type="f" format="type:num;dec:2">Last`AcqCost/Pack</th>
			<th name="lastacqcost_unit" type="f" format="type:num;dec:3">Last`AcqCost/Unit</th>
			<th name="lastacqcost_date" type="i" format="type:date;width:8;dec:0">Last`AcqCost Date</th>
			<th name="onhand_qty" type="f" format="type:nocommas;width:6">On Hand`QTY</th>
			<th name="onhand_value" type="f" format="type:num;dec:2">On Hand`Value</th>
			<th name="commited_qty" type="f" format="type:nocommas;width:6">Comitted`QTY</th>
			<th name="commited_value" type="f" format="type:num;dec:2">Comitted`Value</th>
			<th name="total_qty" type="f" format="type:nocommas;width:6">Total`QTY</th>
			<th name="total_value" type="f" format="type:num;dec:2">Total`Value</th>
			<th name="acq_cost_pack" type="f" format="type:num;dec:2">Acq`Cost/Pack</th>
			<th name="acq_cost_unit" type="f" format="type:num;dec:3">Acq`Cost/Unit</th>
			<th name="prim_supplier" type="a" format="type:char;width:21">Primary`Supplier</th>
			<th name="last_chg_date" type="i" format="type:date;width:8;dec:0">Last`Change Date</th>
		</cols>
		<data>{0}</data>
	</table>
	<name mode="{1}">{2}.{4}.d{3:yyyyMMdd}</name>
	<users>
		<user>wegmans_cjmccarthy</user>
		<user>wegmans_vndtoptaltxf</user>
		<user>wegmans_rx_batch</user>
		<user>wegmans_rx_batch1</user>
		<user>wegmans_rx_batch2</user>
		<user>wegmans_rx_batch_pool</user>
		<user>wegmans_rx_batch_inn</user>
		<user>wegmans_internal</user>
	</users>
</in>