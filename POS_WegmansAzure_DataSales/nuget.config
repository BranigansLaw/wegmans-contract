<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <packageSources>
    <clear />
    <add key="nuget.org" value="https://api.nuget.org/v3/index.json" />
    <add key="EnterpriseLibrary" value="https://pkgs.dev.azure.com/wegmans/_packaging/EnterpriseLibrary/nuget/v3/index.json" />
  </packageSources>
  <packageSourceMapping>
    <packageSource key="nuget.org">
      <package pattern="*" />
    </packageSource>
    <packageSource key="EnterpriseLibrary">
      <package pattern="Wegmans.EnterpriseLibrary" />
      <package pattern="Wegmans.EnterpriseLibrary.*" />
    </packageSource>
  </packageSourceMapping>
</configuration>